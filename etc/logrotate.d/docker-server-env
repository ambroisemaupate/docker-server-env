/root/docker-server-env/bckup_logs/*.log {
    weekly
    missingok
    rotate 7
    compress
    notifempty
    nocreate
}
/root/docker-server-env/compose/traefik/access.log {
    daily
    missingok
    rotate 7
    compress
    notifempty
    create 644 root root
    postrotate
    cd /root/docker-server-env/compose/traefik && /usr/local/bin/docker-compose up -d --force-recreate --no-deps traefik
    endscript
}
