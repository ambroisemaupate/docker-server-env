/root/docker-server-env/bckup_logs/*.log {
    weekly
    missingok
    rotate 7
    compress
    notifempty
    nocreate
}
# Change here your traefik access.log path and user
/root/docker-server-env/compose/traefik/access.log {
    weekly
    missingok
    rotate 7
    compress
    notifempty
    create 644 root root
}
